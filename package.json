    "eslint-plugin-jest-playwright": "0.3.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-testing-library": "4.6.0",
    "expect-playwright": "0.4.1",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-playwright-preset": "1.6.1",
    "lint-staged": "11.0.0",
    "playwright": "1.12.2",
    "plop": "2.7.4",
