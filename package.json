{
  "name": "cna-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest -c jest.unit.config.js",
    "e2e": "jest -c jest.e2e.config.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "lint": "npm run lint:tsc && npm run lint:eslint",
